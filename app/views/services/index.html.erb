<p id="notice"><%= notice %></p>

<h1>Services</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= link_to 'Show', vehicle_service_path(@vehicle, service) %></td>
        <td><%= link_to 'Edit', edit_vehicle_service_path(@vehicle, service) %></td>
        <td><%= link_to 'Destroy', vehicle_service_path(@vehicle, service), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_vehicle_service_path(@vehicle) %> |
<%= link_to 'Back', vehicle_path(@vehicle) %>
